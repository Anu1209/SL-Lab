class person:
x=0
def __init__(self,name,age):
self.name = name
self.age = age
person.x+=1
def __del__(self):
p1=person("Anu", 19)
print ("Name of person is :", p1.name)
print ("Age of person is :",format(p1.age))
del p1.age  #delete the age attribute
print ("Name of person is :", p1.name)
print ("Age of person is :", p1.age)
del p1      #delete the object attribute
print ("Name of person is :", p1.name)
print ("Age of person is :", p1.age)
